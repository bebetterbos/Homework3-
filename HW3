#include <iostream>
using namespace std;
int main()
{
	cout << "********** Grade Report *********\n";
	int score;
	cout << "Input Score : ";
	cin >> score;
	cout << "You got ";
	if (score <= 100 && score >=90)
	{
		cout << "A" << endl;
	}
	else if (score <= 89 && score >=80)
	{
		cout << "B" << endl;
	}
	else if (score <= 79 && score >=70)
	{
		cout << "C" << endl;
	}
	else if (score <= 69 && score >=60)
	{
		cout << "D" << endl;
	}
	else if (score <= 59 && score >=0)
	{
		cout << "F" << endl;
	}
	else 
	{
		cout << "Error, Please input score 0-100" << endl;
	}
	cout << "*********************************\n" ;
	return(0);
}
